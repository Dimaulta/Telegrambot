# БАЗОВЫЕ URL
# URL туннеля от ngrok (стабильный, не меняется неделями)
# Пример: https://b1af2365e793.ngrok-free.app
BASE_URL=https://your-ngrok-url.ngrok-free.app

# ТОКЕНЫ БОТОВ
# Получите токены в @BotFather в Telegram
VIDEO_BOT_TOKEN=123456:REPLACE_ME_VIDEO
NEURFOTOBOT_TOKEN=123456:REPLACE_ME_NEURFOTOBOT
GSFORTEXTBOT_TOKEN=123456:REPLACE_ME_GSFORTEXTBOT
WMMOVEBOT_TOKEN=123456:REPLACE_ME_WMMOVEBOT
NOWMTTBOT_TOKEN=123456:REPLACE_ME_NOWMTTBOT
SORANOWBOT_TOKEN=123456:REPLACE_ME_SORANOWBOT
BANANANOWBOT_TOKEN=123456:REPLACE_ME_BANANANOWBOT
VEONOWBOT_TOKEN=123456:REPLACE_ME_VEONOWBOT

# ПОРТЫ СЕРВИСОВ (если нужно переопределить)
BANANANOWBOT_PORT=8088
VEONOWBOT_PORT=8087

# API-ключи для Nano Banana
BANANANOWBOT_NANO_API_KEY=replace-with-your-nano-banana-api-key

# ПАРАМЕТРЫ ДЛЯ VEO 3 API
VEONOWBOT_WEBHOOK_SECRET=
VEO3_API_KEY=
VEO3_WORKFLOW_ID=
VEO3_BASE_URL=https://api.veo.com/v3
VEONOWBOT_OUTBOUND_PROXY_URL=
VEONOWBOT_OUTBOUND_PROXY_USER=
VEONOWBOT_OUTBOUND_PROXY_PASS=

# ДОПОЛНИТЕЛЬНЫЕ НАСТРОЙКИ (опционально)
TEMP_DIR=Resources/temporaryvideoFiles

# ТОКЕН АВТОРИЗАЦИИ SORA (опционально)
# Получи через DevTools → Network → session → Response Headers → Set-Cookie
# Формат: __Secure-next-auth.session-token=eyJhbGciOiJkaXIiLCJlbmMiOiJBMjU2R0NNIn0...
SORA_SESSION_TOKEN=

# ПАРАМЕТРЫ ВНЕШНЕГО API ДЛЯ SORANOWBOT
# Пример: https://api.example.com/v1/videos
SORANOWBOT_API_URL=https://api.example.com/v1/videos
SORANOWBOT_API_KEY=replace-with-your-api-key
# Прокси на случай, если внешний API недоступен напрямую (опционально)
SORANOWBOT_HTTP_PROXY=
SORANOWBOT_PROXY_USER=
SORANOWBOT_PROXY_PASS=
